/* 帮助 文档   意见反馈    关于 */
.center_help_active {
  margin-top: 10rpx;
}
.center_help {
  width: 750rpx;
  height: 104rpx;
  border-bottom: 1px solid #f5f5f5;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #ffffff;
}
.center_help:last-child {
  border-bottom: 0px solid #f5f5f5;
}
.center_help view {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  padding-left: 32rpx;
  color: #333333;
}
.center_help image {
  width: 16rpx;
  height: 26rpx;
  margin-right: 32rpx;
}
.icon_navito {
  width: 750rpx;
  height: 144rpx;
  background: #ffffff;
  box-shadow: 0px 2px 4px 0px #f5f5f5;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.icon_navito .icon_navito_data {
  width: 187.5rpx;
  height: 144rpx;
  text-align: center;
  padding-top: 35rpx;
  box-sizing: border-box;
}
.icon_navito .icon_navito_data image {
  width: 42rpx;
  height: 42rpx;
}
.icon_navito .icon_navito_data view {
  text-align: center;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #4a4a4a;
}
.header {
  width: 750rpx;
  height: 224rpx;
  background: #4a4a4a;
}
.header .header_dt {
  width: 750rpx;
  height: 224rpx;
  padding-top: 40rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.header .header_dt image {
  width: 144rpx;
  height: 144rpx;
