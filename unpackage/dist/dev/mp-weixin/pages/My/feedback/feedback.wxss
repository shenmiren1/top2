/* 提交成功显示 */
.succeed {
  width: 750rpx;
  height: 100vh;
  background-color: #FFFFFF;
  z-index: 10;
  position: fixed;
  top: 0;
  left: 0;
}
.succeed .succeed_alert {
  width: 670rpx;
  height: 42vh;
  padding: 10vh  40rpx 0   40rpx;
}
.succeed .succeed_icon {
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding-top: 10vh;
}
.succeed .succeed_icon image {
  width: 100rpx;
  height: 100rpx;
}
.succeed .succeed_icon_text {
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 26rpx;
  font-weight: 400;
  color: #000000;
  padding-top: 20rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
}
.commit_tj {
  width: 750rpx;
  height: 70rpx;
  margin-top: 64rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.commit_tj view {
  width: 600rpx;
  height: 70rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #f08e2d;
  border-radius: 35rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ffffff;
}
.hi_logo {
  width: 657rpx;
  height: 463rpx;
  margin: 49rpx 54rpx 11rpx 39rpx;
}
.up_sh {
  width: 100rpx;
  height: 100rpx;
  background: #f5f6f8;
  border: 1rpx solid #c3c0c4;
  margin-left: 40rpx;
}
.up_sh image {
  width: 100rpx;
  height: 100rpx;
  background: #f5f6f8;
  border: 1rpx solid #c3c0c4;
}
.contne {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  margin-left: 40rpx;
  margin-top: 30rpx;
