.honor {
  width: 750rpx;
  height: auto;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #ffffff;
}
.honor .honor_boton {
  height: 70rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #f08e2d;
}
.honor .honor_listData {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #000000;
}
.honor .honor_listData:nth-child(2) {
  padding-top: 53 -38rpx;
}
.honor .honor_listData .honor_listData_year {
  padding-top: 38rpx;
  padding-right: 18rpx;
}
.honor .honor_listData .honor_listData_data {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 84%;
  margin-left: 104rpx;
}
.honor .honor_listData .honor_listData_data:nth-child(2) {
  margin-left: 0;
  padding-top: 0;
}
.honor .honor_listData .honor_listData_data:nth-child(2) .yuan {
  background-color: #f08e2d;
}
.honor .honor_listData .honor_listData_data .yuan {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  border: 2rpx solid #f08e2d;
  margin-top: 48rpx;
}
.honor .honor_listData .honor_listData_data .honor_listData_data_data {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 100%;
}
.honor .honor_listData .honor_listData_data .honor_listData_data_data .honor_listData_data_name {
  padding-left: 28rpx;
  padding-top: 38rpx;
  width: 460rpx;
}
.honor .honor_listData .honor_listData_data .honor_listData_data_data .honor_listData_data_name view:nth-child(2) {
  color: #f08e2d;
}
.honor .honor_listData .honor_listData_data .honor_listData_data_data .honor_listData_data_zy {
  padding-top: 38rpx;
}
.honor .honor_title {
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: #000000;
  padding-bottom: 6rpx;
  border-bottom: 2rpx solid #EEEEEE;
}

