.car {
  position: fixed;
  top: 0;
  width: 750rpx;
  background-color: #fff;
  overflow: hidden;
}
.car .car_cah {
  height: 40rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.car .car_cah view {
  margin-right: 40rpx;
}
.footer {
  background-color: #fff;
  margin-top: 10rpx;
}
.footer .sp_data {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 20rpx;
  text-align: center;
}
.footer .sp_data view {
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  border: 1px solid #B7B7B7;
  color: #B7B7B7;
  margin-left: 120rpx;
}
.footer .sp_data view:nth-child(2) {
  margin-left: 10rpx;
}
.footer .sp_data view:nth-child(3) {
  margin-left: 10rpx;
}
.footer .sp_data .active {
  color: #F08E2D;
  border: 1px solid #F08E2D;
}
.footer .zh_data {
  font-size: 34rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: #000000;
  margin: 50rpx 0 24rpx 50rpx;
}
.footer .zh_mg {
  margin: 120rpx 0 24rpx 50rpx;
}
.footer .xk_tab {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 20rpx;
  text-align: center;
}
.footer .xk_tab view {
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
