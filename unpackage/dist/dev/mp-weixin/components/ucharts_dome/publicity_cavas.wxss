  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 50rpx;
  padding-top: 30rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.footer .footer_name view:nth-child(1) {
  font-size: 34rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: #f08e2d;
}
.footer .footer_name view:nth-child(2) {
  width: 190rpx;
  height: 40rpx;
  background: #d8d8d8;
  border: 1px solid #979797;
  box-sizing: border-box;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #4a4a4a;
  padding-left: 16rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.footer .footer_name view:nth-child(2) image {
  width: 15rpx;
  height: 8rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
}
