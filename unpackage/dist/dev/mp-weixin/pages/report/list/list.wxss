page {
  background-color: #fff;
  padding-bottom: 150rpx;
}
.canvas_view {
  padding: 40rpx 0;
}
.charts {
  width: 750rpx;
  height: 500rpx;
  background-color: #ffffff;
}
.register {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.register view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 450rpx;
  height: 100rpx;
  border: 1px solid #007aff;
  font-size: 30rpx;
  color: #007aff;
  margin-top: 50rpx;
}
.hader_center {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-left: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #000000;
  padding-top: 19rpx;
}
.hader_center .hader_center_top {
  margin-left: 30rpx;
}
.hader_center .name_border {
  border: 2rpx solid #f08e2d;
  color: #000000;
  padding: 10rpx;
  background-color: #F08E2D;
}
.hader_center .hader_center_pos {
  position: absolute;
  background-color: #fff;
  top: 56rpx;
  z-index: 10;
}
.hader_center .hader_center_pos .nn_cover-view {
  width: 112rpx;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #000000;
  margin: 20rpx 24rpx;
}
.hader_center .hader_center_pos .nn_cover-view:nth-child(2) {
  border-top: 1px solid #eeeeee;
}
.hader_center .hader_center_pos .active {
  color: #f08e2d;
}
.hader_center image {
  width: 16rpx;
  height: 16rpx;
  margin-left: 10rpx;
}
.hader_center view:nth-child(1) {
  margin: 0;
  color: #fff;
}
.hader_center view:nth-child(2) {
  color: #fff;
}
.hader_center view:nth-child(3) {
  color: #fff;
}
.fiexd {
  position: fixed;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: 98rpx;
  width: 750rpx;
  border: 1px solid #dbd9dc;
  background-color: #fff;
}
.fiexd input {
  width: 376rpx;
  height: 60rpx;
  background: #f5f6f8;
  border-radius: 30px;
  font-size: 24rpx;
  margin-left: 28rpx;
  padding-left: 56rpx;
}
.fiexd image {
  width: 40rpx;
  height: 40rpx;
}
.fiexd image:last-child {
  position: absolute;
  width: 20rpx;
  height: 20rpx;
  left: 53rpx;
}
.read_data {
  padding-left: 52rpx;
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #8e8e8e;
}
.image {
  padding-top: 40rpx;
  padding: 40rpx 52rpx 41rpx 52rpx;
}
.image image {
  width: 669rpx;
  height: 669rpx;
}
.conter {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #000000;
  padding-left: 52rpx;
  padding-right: 52rpx;
  padding-top: 42rpx;
}
.header_time {
  width: 750rpx;
  padding-left: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 20rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: #8e8e8e;
}
.header_time view:nth-child(2) {
