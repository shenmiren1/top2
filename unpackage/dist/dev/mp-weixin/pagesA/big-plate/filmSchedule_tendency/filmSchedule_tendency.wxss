/* 城市搜索 */
.city_sel {
  width: 750rpx;
  height: 100%;
  position: fixed;
  left: 0;
  z-index: 10;
  background-color: #fff;
}
.city_sel .placeholder {
  font-size: 28rpx;
  margin: 10rpx;
  text-align: center;
  background-color: #fff;
  height: 50rpx;
  line-height: 50rpx;
  color: #000;
  border: 1rpx solid #ccc;
}
.searchbar-result {
  margin-top: 0;
  font-size: 28rpx;
}
.searchbar-result:before {
  display: none;
}
.city_sel .weui-cell {
  padding: 24rpx 30rpx 24rpx 70rpx;
}
.city_sel .title {
  font-size: 24rpx;
  color: #333;
  padding: 20rpx 0;
  padding-left: 20rpx;
}
.city_sel .weui-grid__label {
  margin-top: 0;
  font-size: 24rpx;
  padding: 5rpx 0;
}
.city_sel .weui-grid {
  padding: 0;
  width: 28%;
  margin-right: 6%;
  margin-bottom: 20rpx;
  border: 1rpx solid #ccc;
}
.city_sel .weui-grid:nth-child(3) {
  padding: 0;
  width: 28%;
  margin-right: 0%;
}
.city_sel .weui-grid:nth-child(6) {
  padding: 0;
  width: 28%;
  margin-right: 0%;
}
.city_sel .weui-grid:nth-child(9) {
  padding: 0;
  width: 28%;
  margin-right: 0%;
}
.city_sel .page__bd {
  padding: 0 20rpx;
}
.city_sel .weui-grids {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  border: none;
  text-align: center;
}
.alphanet-selector {
  position: absolute;
  top: 16rpx;
  right: 0;
  height: 1334rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  z-index: 1000;
  box-sizing: border-box;
}
.alphanet-selector .selector-one {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 24rpx;
  color: #43c1f4;
  height: 35rpx;
}
/* 城市搜索 结束*/
.page__view {
  height: 1334rpx;
}
.page__view .page__view_cha {
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
